    scrollContainer.ScrollingDirection = Enum.ScrollingDirection.Y -- ä»…å‚ç›´æ»šåŠ¨
    scrollContainer.CanvasSize = UDim2.new(1, 0, 0, 0) -- åŠ¨æ€è®¡ç®—
    scrollContainer.Parent = dialogFrame
    createCorner(scrollContainer, 8)
    
